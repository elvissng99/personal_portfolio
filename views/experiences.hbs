<div class = "container">
    {{#if experiences.length}}
        <div class="experiencesContainer">
            <h1>Experiences</h1>
            {{#each experiences}}
                <div class = "innerContainer">
                    <div>
                        <h2>{{title}}</h2>
                    </div>
                    <div class = 'fontSize'>
                        <p>{{description}}</p>
                    </div>  
                </div>
            {{/each}}   
        </div>
        
    {{else}}
        <div class = "errormessage">
            <p>NO EXPERIENCES DATA</p>
        </div>
    {{/if}}
</div>