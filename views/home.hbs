<div>
    <div>
        <h2>{{home.title}}</h3>
        <p>{{home.occupation}}</p>
        <p>{{home.description}}</p>
        <br>
    </div>
    {{#if projects.length}}
        <h3>Projects</h3>
        {{#each projects}}
        
        <div>
            <a href = "/project/{{id}}"><img src="{{imagePath}}" alt="{{imagePath}}"></a>
            <p><strong>{{title}}</strong></p>
            {{!-- <p>{{description}}</p> --}}
            <br>
        </div>
        {{/each}}
    {{else}}
    <div>
        <p>NO PROJECTS DATA</p>
    </div>
    {{/if}}
</div>